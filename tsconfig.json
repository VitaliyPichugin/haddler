    "outDir": "./dist/",
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react"
  },
  "include": ["./src/**/*"]
}